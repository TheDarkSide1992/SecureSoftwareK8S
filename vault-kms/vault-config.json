{
  "storage": {
    "file": {
      "path": "/vault/file"
    }
  },
  "listener": {
    "tcp": {
      "address": "[::]:8200",
      "cluster_address": "[::]:8201",
      "tls_disable": false,
      "tls_cert_file": "/vault/certs/vault.crt",
      "tls_key_file": "/vault/certs/vault.key",
      "tls_client_ca_file": "/vault/certs/ca.crt"
    }
  },
  "ui": true
}
